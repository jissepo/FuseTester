{
  "name": "fusetester",
  "version": "1.0.0",
  "description": "64-fuse monitoring system for Raspberry Pi 1 Model B+ using I2C ADC and GPIO multiplexers",
  "main": "src/main.js",
  "scripts": {
    "start": "node src/main.js",
    "dev": "NODE_ENV=development node src/main.js",
    "lint": "biome check src/",
    "format": "biome format src/ --write"
  },
  "keywords": [
    "raspberry-pi",
    "raspberry-pi-1-b+",
    "nodejs",
    "arm6",
    "pi-os",
    "i2c",
    "csv-logging",
    "fuse-monitoring",
    "ads1115",
    "multiplexer",
    "gpio"
  ],
  "author": "jissepo",
  "license": "ISC",
  "engines": {
    "node": ">=24.5.0"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "pigpio": "^3.3.1",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jissepo/FuseTester.git"
  },
  "homepage": "https://github.com/jissepo/FuseTester#readme",
  "bugs": {
    "url": "https://github.com/jissepo/FuseTester/issues"
  },
  "os": [
    "linux"
  ],
  "cpu": [
    "arm"
  ],
  "preferGlobal": false,
  "private": true
}
