{
  "name": "fusetester",
  "version": "1.0.0",
  "description": "I2C data collection and CSV logging for Raspberry Pi 1 Model B+ running Pi OS",
  "main": "src/main.js",
  "scripts": {
    "start": "node src/main.js",
    "dev": "NODE_ENV=development node src/main.js",
    "test": "jest",
    "lint": "biome check src/",
    "format": "biome format src/ --write",
    "install-pi": "sudo ./scripts/install.sh",
    "deploy": "./scripts/deploy.sh"
  },
  "keywords": [
    "raspberry-pi",
    "nodejs",
    "arm6",
    "pi-os",
    "i2c",
    "csv",
    "data-logging"
  ],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "winston": "^3.8.2",
    "pigpio": "^3.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.3",
    "jest": "^29.4.0"
  },
  "os": [
    "linux"
  ],
  "cpu": [
    "arm"
  ]
}
